#include <stdio.h>

int main()
{
        int a[100], n, i, el;
        printf("\nEnter the number of elements in the array: ");
        scanf("%d", &n);
        printf("\nEnter the elements in the array: ");
        for(i=0; i<n; i++)
                scanf("%d", &a[i]);
        printf("\nEnter the element to search: ");
        scanf("%d", &el);
        for(i=0; i<n; i++)
        {
                if(a[i]==el)
                {
                        printf("\nElement found at index %d! ", i);
                }
        }
        return 0;

}
